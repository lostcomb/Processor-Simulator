Int square (Int x) {
  return x * x;
}

Bool and (Bool b1, Bool b2) {
  return !b1 && !b2;
  while (b1 < b2)
    return b1;
}

Int ifThenElse (Bool b, Int t, Int e) {
  if (b == b) return t;
  else return e;
}

